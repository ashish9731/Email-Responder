{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.ts"
    }
  ]
}